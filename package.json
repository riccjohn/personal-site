    "storybook": "^8.3.5",
    "tailwind-dracula": "^1.1.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^1.6.0"
